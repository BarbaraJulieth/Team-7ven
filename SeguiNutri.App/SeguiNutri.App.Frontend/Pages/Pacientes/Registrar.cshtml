@page
@model SeguiNutri.App.Frontend.Pages.Pacientes.RegistrarModel
@{
}
<h1>Nuevo Paciente</h1>
<form method="post">

    <div class="form-group">
        <label>Cedula</label>
        <input asp-for="Paciente.Cedula" class="form-control">
        <span class="text-danger" asp-validation-for="Paciente.Cedula"></span>
    </div>

    <div class="form-group">
        <label>Nombre</label>
        <input asp-for="Paciente.Nombre" class="form-control">
        <span class="text-danger" asp-validation-for="Paciente.Nombre"></span>
    </div>

    <div class="form-group">
        <label>Apellido</label>
        <input asp-for="Paciente.Apellido" class="form-control">
        <span class="text-danger" asp-validation-for="Paciente.Apellido"></span>
    </div>

    <div class="form-group">
        <label>Fecha De Nacimiento</label>
        <input asp-for="Paciente.FechaNacimiento" class="form-control">
    </div>

    <div class="form-group">
        <label>Correo</label>
        <input asp-for="Paciente.Correo" class="form-control">
        <span class="text-danger" asp-validation-for="Paciente.Correo"></span>
    </div>

    <div class="form-group">
        <label>Telefono</label>
        <input asp-for="Paciente.Telefono" class="form-control">
        <span class="text-danger" asp-validation-for="Paciente.Telefono"></span>
    </div>

    <div class="form-group">
        <label>Id Paciente</label>
        <input asp-for="Paciente.IdPaciente" class="form-control">
    </div>

    <div class="form-group">
        <label>Contraseña</label>
        <input asp-for="Paciente.Contraseña" class="form-control">
        <span class="text-danger" asp-validation-for="Paciente.Contraseña"></span>
    </div>

    <button type="submit" class="btn btn-primary table-btn">Registrarse</button>

</form>