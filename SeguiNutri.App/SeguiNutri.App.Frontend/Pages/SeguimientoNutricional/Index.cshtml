@page
@using SeguiNutri.App.Dominio
@model SeguiNutri.App.Frontend.Pages.SeguimientoNutricional.IndexModel
@{
}
<h1>Seguimiento nutricional del Paciente @Model.paciente.Nombre  @Model.paciente.Apellido</h1>

<form method="post">
    <div class="form-group top">
        <div class="form-group">
            <label>Id Nutricionista</label>
            <input asp-for="SeguimientoNutris.IdNutricionista" class="form-control">        
        </div>     
        <div class="form-group">   
            <label>Id Coach</label>
            <input asp-for="SeguimientoNutris.IdCoach" class="form-control">        
        </div>     
    </div>        
        <div class="form-group sug">
            <div><label>Sugerencias de Cuidados</label></div>
            <input type="text" asp-for="SeguimientoNutris.SugerenciasCuid" class="form-control">
        </div>
    <button type="submit" class="btn btn-primary">Registro</button>

</form>